{
  "env": {
    "NODE_ENV": "production",
    "DOMAIN": "citizenspring.be",
    "BASE_URL": "https://citizenspring.be",
    "DEBUG": "webhook,email",
    "MAILGUN_USER": "postmaster@citizenspring.be",
    "MAILGUN_API_KEY": "@mailgun_api_key",
    "MAILGUN_PASSWORD": "@mailgun_password",
    "PG_URL": "@pg_url",
    "PG_DATABASE": "@pg_database",
    "PG_HOST": "@pg_host",
    "PG_USERNAME": "@pg_username",
    "PG_PASSWORD": "@pg_password"
  },
  "public": true,
  "version": 2,
  "alias": ["citizenspring.be", "www.citizenspring.be"],
  "builds": [
    { "src": "build/server/controllers/webhook.js", "use": "@now/node" },
    { "src": "build/server/index.js", "use": "@now/node-server" },
    { "src": "build/static/**", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/webhook", "methods": ["POST"], "dest": "/build/server/controllers/webhook.js" },
    { "src": "/static/(.*)", "methods": ["GET"], "dest": "/build/static/$1" },
    { "src": "(.*)", "methods": ["GET"], "dest": "/build/server/index.js" }
  ],
  "regions": ["bru"]
}
