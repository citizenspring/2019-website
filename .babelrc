{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "auto"
        }
      }
    ]
  ],
  "plugins": [
    "lodash",
    [
      "react-intl",
      {
        "messagesDir": "./build/messages/"
      }
    ],
    ["styled-components", { "ssr": true }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }]
  ],
  "env": {
    "development": {
      "compact": false
    }
  }
}
