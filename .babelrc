{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "auto"
        }
      }
    ]
  ],
  "plugins": [
    "add-module-exports",
    "lodash",
    [
      "react-intl",
      {
        "messagesDir": "./build/messages/"
      }
    ],
    "styled-components"
  ],
  "env": {
    "development": {
      "compact": false
    }
  }
}
